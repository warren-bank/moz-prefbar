    "onclick":"BrowserOpenTab();"
  },
  "prefbar:button:prefbarrestoretab":{
    "label":"Restore Tab",
    "type":"button",
    "onclick":"prefbarRestoreTab();"
  },
  "prefbar:button:killflash":{
    "label":"Kill Flash",
    "type":"button",
    "onclick":"prefbarKillFlash()"
  },
  "prefbar:button:separator12":{
    "type":"separator"
  },
  "prefbar:button:fontdown":{
    "label":"Font -",
    "type":"button",
